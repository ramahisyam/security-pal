<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IdcardDetailSubcon">
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/main_employee_fab"
            android:layout_gravity="bottom|end"
            android:layout_margin="16dp"
            android:src="@drawable/add"
            app:maxImageSize="30dp"
            android:backgroundTint="#8BC34A"
            app:rippleColor="#8BC34A"/>
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#000000"
            android:id="@+id/main_employee_fab1"
            android:layout_gravity="bottom|end"
            android:layout_marginBottom="90dp"
            android:layout_marginEnd="16dp"
            android:src="@drawable/pdf"
            android:visibility="invisible"
            android:tooltipText="Download Excel"
            app:rippleColor="#9C27B0"/>
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#E91E63"
            android:id="@+id/main_employee_fab2"
            android:layout_gravity="bottom|end"
            android:layout_marginEnd="90dp"
            android:layout_marginBottom="16dp"
            android:src="@drawable/folder"
            android:visibility="invisible"
            android:tooltipText="Excel Folder"
            app:rippleColor="#FFEB3B"
            tools:ignore="MissingConstraints" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/bgidcard"
            android:id="@+id/lld">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/bumn"
                    android:layout_width="230dp"
                    android:layout_height="35dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:src="@drawable/bumn">
                </ImageView>
                <ImageView
                    android:id="@+id/defend"
                    android:layout_width="160dp"
                    android:layout_height="30dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/defend"
                    app:layout_constraintTop_toBottomOf="@id/bumn">
                </ImageView>
                <ImageView
                    android:id="@+id/avatarview"
                    android:layout_width="136sp"
                    android:layout_height="178sp"
                    android:src="@drawable/ic_baseline_image_24"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/defend"
                    android:layout_marginTop="20dp"/>

                <TextView
                    android:id="@+id/namedocsub"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/gabriela"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:text="Name"
                    android:textColor="@color/black"
                    android:textSize="20dp" />
                <TextView
                    android:id="@+id/tvid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/avatarview"
                    android:text="SUBCONTRACTOR"
                    android:gravity="center"
                    android:fontFamily="@font/gabriela"
                    android:textColor="#174887"
                    android:layout_marginTop="5dp"
                    android:textSize="20dp"/>
                <LinearLayout
                    android:id="@+id/ll2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="30dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tvid">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview2"
                        android:text="Company"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/comdocsub"
                        android:layout_width="210dp"
                        android:layout_height="wrap_content"
                        android:text="Company"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>
                </LinearLayout>
<!--                <LinearLayout-->
<!--                    android:id="@+id/ll3"-->
<!--                    android:layout_width="wrap_content"-->
<!--                    android:layout_height="wrap_content"-->
<!--                    android:orientation="horizontal"-->
<!--                    android:layout_marginTop="5dp"-->
<!--                    app:layout_constraintEnd_toEndOf="parent"-->
<!--                    app:layout_constraintStart_toStartOf="parent"-->
<!--                    app:layout_constraintTop_toBottomOf="@id/ll2">-->
<!--                    <TextView-->
<!--                        android:textStyle="bold"-->
<!--                        android:textSize="17dp"-->
<!--                        android:layout_width="105dp"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        android:id="@+id/textview3"-->
<!--                        android:text="Division"-->
<!--                        android:textColor="@color/black"/>-->

<!--                    <TextView-->
<!--                        android:id="@+id/divdocsub"-->
<!--                        android:layout_width="210dp"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        android:text="Division"-->
<!--                        android:textAlignment="textStart"-->
<!--                        android:textSize="17dp"-->
<!--                        android:textColor="@color/black"/>-->
<!--                </LinearLayout>-->
                <LinearLayout
                    android:id="@+id/ll4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll2">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview4"
                        android:text="Age"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/agedocsub"
                        android:layout_width="210dp"
                        android:layout_height="wrap_content"
                        android:text="Age"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll4">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview5"
                        android:text="NIP"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/nipdocsub"
                        android:layout_width="210dp"
                        android:layout_height="wrap_content"
                        android:text="NIP"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll5">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/avatarview"
                        android:id="@+id/textview6"
                        android:text="Phone"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/phonedocsub"
                        android:layout_width="210dp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/avatarview"
                        android:layout_toRightOf="@+id/textview2"
                        android:text="Phone"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll6">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview7"
                        android:text="Address"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/adddocsub"
                        android:layout_width="210dp"
                        android:layout_height="wrap_content"
                        android:text="Address"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll7">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="20dp"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview8"
                        android:text="Join"
                        android:textColor="#8BC34A"/>

                    <TextView
                        android:id="@+id/startdocsub"
                        android:textStyle="bold"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="Join"
                        android:textAlignment="textStart"
                        android:textSize="20dp"
                        android:textColor="#8BC34A"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll8">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="20dp"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview9"
                        android:text="Exp"
                        android:textColor="#E91E1E"/>

                    <TextView
                        android:id="@+id/finishdocsub"
                        android:textStyle="bold"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="Exp"
                        android:textAlignment="textStart"
                        android:textSize="20dp"
                        android:textColor="#E91E1E"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|start"
                    android:orientation="vertical"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="40dp">
                    <ImageView
                        android:layout_width="85dp"
                        android:layout_height="85dp"
                        android:src="@drawable/qrhcm" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</androidx.constraintlayout.widget.ConstraintLayout>