<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IdcardDetailGuest">

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/main_employee_fab"
            android:layout_gravity="bottom|end"
            android:layout_margin="16dp"
            android:src="@drawable/add"
            app:maxImageSize="30dp"
            android:backgroundTint="#8BC34A"
            app:rippleColor="#8BC34A"/>
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#000000"
            android:id="@+id/main_employee_fab1"
            android:layout_gravity="bottom|end"
            android:layout_marginBottom="90dp"
            android:layout_marginEnd="16dp"
            android:src="@drawable/pdf"
            android:visibility="invisible"
            android:tooltipText="Download Excel"
            app:rippleColor="#9C27B0"/>
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#E91E63"
            android:id="@+id/main_employee_fab2"
            android:layout_gravity="bottom|end"
            android:layout_marginEnd="90dp"
            android:layout_marginBottom="16dp"
            android:src="@drawable/folder"
            android:visibility="invisible"
            android:tooltipText="Excel Folder"
            app:rippleColor="#FFEB3B"
            tools:ignore="MissingConstraints" />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/bgpdfguest1"
            android:id="@+id/lld">
                <ImageView
                    android:id="@+id/bumn"
                    android:layout_width="280dp"
                    android:layout_height="35dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="20dp"
                    android:src="@drawable/bumn">
                </ImageView>
                <ImageView
                    android:id="@+id/defend"
                    android:layout_width="200dp"
                    android:layout_height="35dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/defend"
                    app:layout_constraintTop_toBottomOf="@id/bumn">
                </ImageView>
                <LinearLayout
                    android:id="@+id/ll11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:layout_marginTop="40dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/defend">
                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:src="@drawable/qrhcm" />
                </LinearLayout>
                <TextView
                    android:id="@+id/namedocgu"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/gabriela"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:text="Name"
                    android:textColor="@color/black"
                    android:textSize="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll11"/>
                <TextView
                    android:id="@+id/tvid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/namedocgu"
                    android:text="GUEST"
                    android:gravity="center"
                    android:fontFamily="@font/gabriela"
                    android:textColor="#174887"
                    android:layout_marginTop="5dp"
                    android:textSize="20dp"/>
                <LinearLayout
                    android:id="@+id/ll2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="30dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tvid">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview2"
                        android:text="Company\t\t\t  :"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/comdocgu"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:text="Company"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll2">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/avatarview"
                        android:id="@+id/textview6"
                        android:text="Phone\t\t\t\t\t\t  :"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/phonedocgu"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/avatarview"
                        android:layout_toRightOf="@+id/textview2"
                        android:text="Phone"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll3">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview3"
                        android:text="Division\t\t\t\t  :"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/divdocgu"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:text="Division"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>

                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll4">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview4"
                        android:text="Department\t:"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/depdocgu"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:text="Department"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>

                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll5">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview7"
                        android:text="PIC\t\t\t\t\t\t\t\t\t  :"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/picdocgu"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:text="PIC"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll6">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:layout_width="105dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview8"
                        android:text="Necessity\t\t\t  :"
                        android:textColor="@color/black"/>

                    <TextView
                        android:id="@+id/necdocgu"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:text="Necessity"
                        android:textAlignment="textStart"
                        android:textSize="17dp"
                        android:textColor="@color/black"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="30dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll7">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="20dp"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview12"
                        android:text="Date"
                        android:textColor="#174887"/>
                    <TextView
                        android:id="@+id/datedocgu"
                        android:textStyle="bold"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="Date"
                        android:textAlignment="textStart"
                        android:textSize="20dp"
                        android:textColor="#174887"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll8">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="20dp"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview11"
                        android:text="In"
                        android:textColor="#174887"/>

                    <TextView
                        android:id="@+id/startdocgu"
                        android:layout_width="150dp"
                        android:layout_height="match_parent"
                        android:text="In"
                        android:textAlignment="textStart"
                        android:textColor="#174887"
                        android:textSize="20dp"
                        android:textStyle="bold" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll9">
                    <TextView
                        android:textStyle="bold"
                        android:textSize="20dp"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textview9"
                        android:text="Out"
                        android:textColor="#174887"/>

                    <TextView
                        android:id="@+id/finishdocgu"
                        android:textStyle="bold"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="Out"
                        android:textAlignment="textStart"
                        android:textSize="20dp"
                        android:textColor="#174887"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="PT. PAL INDONESIA"
                        android:textStyle="bold"
                        android:fontFamily="@font/gabriela"
                        android:textSize="25dp"
                        android:textColor="@color/white"/>
                </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</androidx.constraintlayout.widget.ConstraintLayout>